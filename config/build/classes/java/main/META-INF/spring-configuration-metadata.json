{
  "groups": [
    {
      "name": "auth",
      "type": "com.auth.AuthProperties",
      "sourceType": "com.auth.AuthProperties"
    },
    {
      "name": "auth.jwt",
      "type": "com.auth.jwt.AuthJwtProperties",
      "sourceType": "com.auth.jwt.AuthJwtProperties"
    }
  ],
  "properties": [
    {
      "name": "auth.auto-security",
      "type": "java.lang.Boolean",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": true
    },
    {
      "name": "auth.bearer-prefix",
      "type": "java.lang.String",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": "Bearer "
    },
    {
      "name": "auth.endpoints-enabled",
      "type": "java.lang.Boolean",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": true
    },
    {
      "name": "auth.jwt.access-seconds",
      "type": "java.lang.Long",
      "description": "Access Token의 유효 기간(초 단위)입니다. 기본값은 900초(15분)입니다.",
      "sourceType": "com.auth.jwt.AuthJwtProperties",
      "defaultValue": 0
    },
    {
      "name": "auth.jwt.refresh-seconds",
      "type": "java.lang.Long",
      "description": "Refresh Token의 유효 기간(초 단위)입니다. 기본값은 1,209,600초(14일)입니다.",
      "sourceType": "com.auth.jwt.AuthJwtProperties",
      "defaultValue": 0
    },
    {
      "name": "auth.jwt.secret",
      "type": "java.lang.String",
      "description": "JWT 서명에 사용할 비밀키입니다. 보안을 위해 HS256 알고리즘 기준 최소 32바이트 이상의 문자열을 권장합니다.",
      "sourceType": "com.auth.jwt.AuthJwtProperties"
    },
    {
      "name": "auth.refresh-cookie-enabled",
      "type": "java.lang.Boolean",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": true
    },
    {
      "name": "auth.refresh-cookie-http-only",
      "type": "java.lang.Boolean",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": true
    },
    {
      "name": "auth.refresh-cookie-name",
      "type": "java.lang.String",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": "refresh_token"
    },
    {
      "name": "auth.refresh-cookie-path",
      "type": "java.lang.String",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": "\/"
    },
    {
      "name": "auth.refresh-cookie-same-site",
      "type": "java.lang.String",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": "Lax"
    },
    {
      "name": "auth.refresh-cookie-secure",
      "type": "java.lang.Boolean",
      "sourceType": "com.auth.AuthProperties",
      "defaultValue": true
    }
  ],
